generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model ExchangeStatementEntry {
  EntryId             Int    @id @default(autoincrement())
  SourceCurrency      String
  DestinationCurrency String
  BuyPrice            Float
  SellPrice           Float
  CapAmount           Int
}
